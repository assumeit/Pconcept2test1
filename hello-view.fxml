<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="320.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.test1.HelloController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label text="Welcome to Login Page" />
    <Label text="Username" />
    <TextField fx:id="usernameField" />
    <Label text="Password" />
    <PasswordField fx:id="passwordField" />
    <Button text="Login" onAction="#onLoginButtonClick" />
    <Label fx:id="welcomeText" />
</VBox>
